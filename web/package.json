{
  "private": true,
  "name": "@matejkosiarcik/web",
  "scripts": {
    "build": "run-s clean build:webpack build:purgecss build:rcs build:minify-css build:svgo build:hash build:gulp build:minify-html build:urllist build:minify-sitemap build:sh",
    "build:webpack": "cross-env NODE_ENV=production webpack --mode production",
    "build:purgecss": "purgecss --config purgecss.config.js",
    "build:rcs": "node rename-selectors.js",
    "build:gulp": "gulp sri",
    "build:minify-css": "npm run --prefix remynify build",
    "build:minify-html": "html-minifier --input-dir public --output-dir public --file-ext html --use-short-doctype --remove-comments --process-conditional-comments --collapse-whitespace --collapse-boolean-attributes --remove-redundant-attributes --remove-script-type-attributes --remove-style-link-type-attributes --sort-class-name --sort-attributes --minify-css true --minify-js true --minify-urls true",
    "build:urllist": "sitemap2urllist --file public/sitemap.xml --output public/urllist.txt",
    "build:minify-sitemap": "run-s build:minify-sitemap:generate build:minify-sitemap:copy",
    "build:minify-sitemap:generate": "sed -E 's~^ +~~' <public/sitemap.xml | tr -d '\n' >public/sitemap-tmp.xml",
    "build:minify-sitemap:copy": "mv public/sitemap-tmp.xml public/sitemap.xml",
    "build:svgo": "run-s build:svgo:proper build:svgo:post",
    "build:svgo:proper": "svgo --folder public --recursive --precision=1 --multipass --quiet",
    "build:svgo:post": "find public -type f -iname '*.svg' -print0 | xargs -0 -n1 sh -c 'tmpfile=\"$(dirname \"${1}\")/$(basename \"${1}\" .svg).tmp.svg\" && sed \"s~<defs/>~~g\" <\"${1}\" | tr -d \"\\n\" >\"${tmpfile}\" && mv \"${tmpfile}\" \"${1}\"' -",
    "build:hash": "node hash.js",
    "build:sh": "sh build.sh",
    "prebuild": "run-s prebuild:goat",
    "prebuild:goat": "node fetch-goatcounter.js",
    "postbuild": "node validate-build.js",
    "clean": "rimraf public",
    "lint": "run-p lint:*",
    "lint:ts": "tsc --noEmit --skipLibCheck",
    "start": "run-p serve",
    "serve-old": "cross-env NODE_ENV=development webpack-dev-server -d --mode development",
    "serve": "cross-env NODE_ENV=development webpack serve --mode development"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "colorette": "1.2.2",
    "core-js": "^3.9.1",
    "hamburgers": "^1.1.3",
    "instant.page": "^5.1.0",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-numeric-separator": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-typescript": "^7.13.0",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "concat": "^1.0.3",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.0",
    "css-validator": "^0.9.0",
    "csslint": "^1.0.5",
    "execa": "^5.0.0",
    "glob": "^7.1.6",
    "glob-all": "^3.2.1",
    "gulp": "^4.0.2",
    "gulp-sri-hash": "^2.2.1",
    "hasha": "^5.2.2",
    "html-loader": "^1.3.2",
    "html-minifier": "^4.0.0",
    "html-validator": "^5.1.17",
    "html-webpack-plugin": "^4.5.2",
    "isomorphic-fetch": "^3.0.0",
    "jshint": "^2.12.0",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass-glob-importer": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.6",
    "postcss-calc": "^8.0.0",
    "postcss-color-hex-alpha": "^7.0.0",
    "postcss-custom-media": "^8.0.0",
    "postcss-custom-properties": "^11.0.0",
    "postcss-functions": "^4.0.2",
    "postcss-hexrgba": "^2.0.1",
    "postcss-loader": "^4.2.0",
    "postcss-media-minmax": "^5.0.0",
    "posthtml-attrs-sorter": "^1.1.0",
    "posthtml-loader": "^2.0.1",
    "posthtml-outlinks": "^1.0.0",
    "posthtml-transform": "^1.0.10",
    "purgecss": "^4.0.2",
    "rcs-core": "^3.6.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "sitemap2urllist-cli": "^0.1.6",
    "style-loader": "^2.0.0",
    "svgo": "^2.1.0",
    "terser-webpack-plugin": "^4.2.3",
    "typescript": "^4.2.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.5.0",
    "webpack-common-shake": "^2.1.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-fix-style-only-entries": "^0.6.1"
  },
  "optionalDependencies": {
    "colorette": "1.2.2"
  }
}
