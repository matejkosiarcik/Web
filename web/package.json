{
  "private": true,
  "scripts": {
    "prebuild": "node favicon.js",
    "build": "run-s clean prebuild build:webpack build:apache build:robots",
    "build:webpack": "cross-env NODE_ENV=production webpack -p --mode production",
    "build:apache": "concat config/.htaccess node_modules/apache-server-configs/dist/.htaccess --output public/.htaccess",
    "build:robots": "concat jekyll/_site/robots.txt config/robots.txt --output public/robots.txt",
    "clean": "rimraf public",
    "lint:css": "stylelint \"style/**/*.{css,scss}\"",
    "lint:html": "htmllint \"jekyll/{_includes,_layouts,_pages,_posts}/**/*.html\"",
    "lint:html2": "htmlhint \"jekyll/{_includes,_layouts,_pages,_posts}/**/*.html\"",
    "lint:js-tsc": "tsc --noEmit --skipLibCheck",
    "lint:js": "eslint --max-warnings 0 \"script/**/*.{ts,js}\"",
    "lint": "run-p lint:*",
    "start": "run-s prebuild serve",
    "serve": "cross-env NODE_ENV=development webpack-dev-server -d"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "core-js": "^3.6.5",
    "hamburgers": "^1.1.3",
